public class SampleTest {

	public static void main(String[] args) {
		ArrayList<Integer> l1 = new ArrayList<Integer>();
		l1.add(10);
		l1.add(20);
		
		ArrayList<Integer> l2 = new ArrayList<Integer>();
		l2.add(30);
		l2.add(40);
		
		Collection.addAll(l1);
	}

}
